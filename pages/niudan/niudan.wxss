.container {
  height: 100%;
  width: 100%;
  overflow: hidden;
  position: relative;
}
.bg-container {
  height: 100%;
  width: 100%;
}
.bg-container image {
  width: 100%;
  height: auto;
}
.times {
  position: absolute;
  top: 100rpx;
}
.start-btn {
  width: 165rpx;
  height: 155rpx;
  position: absolute;
  top: 840rpx;
  left: 142rpx;
  transition: all 1s;
  transform: rotate(0deg);
  z-index: 50;
}
.start-btn .btn-image {
  width: 100%;
  height: 100%;
}
.start {
  animation: startBtn 1s linear;
}
.hide {
  display: none;
}
.egg-exit-door {
  width: 224rpx;
  height: 224rpx;
  position: absolute;
  top: 818rpx;
  right: 135rpx;
  background-image: url(https://2018t3.oss-cn-beijing.aliyuncs.com/polar-bear/images/open.png);
  background-position: center center;
  background-repeat: no-repeat;
  background-size: 100% 100%;
}
.poupResult {
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: 60;
}
.poupResult.scale {
  -webkit-animation: scaleResult .5s linear;
  animation: scaleResult .5s linear;
  -webkit-animation-fill-mode: forwards;
  animation-fill-mode: forwards;
}
.mask {
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: 59;
  background-color: rgba(0, 0, 0, 0.8);
}
.luckEgg,
.showEgg {
  top: 50%;
  left: 50%;
  position: absolute;
  width: 130rpx;
  height: 130rpx;
  -webkit-transform: translate(-50%, -52%) rotateZ(-45deg);
  transform: translate(-50%, -52%) rotateZ(-45deg);
}
.showEgg {
  -webkit-border-radius: 50% 50%;
  border-radius: 50% 50%;
  -webkit-transform: translate(-50%, -52%);
  transform: translate(-50%, -52%);
  overflow: hidden;
}
.showEgg #inner {
  width: 100%;
  height: 100%;
  position: absolute;
  background-size: 100% 100%;
  -webkit-transform: translateY(-110%);
  transform: translateY(-110%);
}
.showEgg.show #inner {
  -webkit-animation: showForResult .5s linear;
  animation: showForResult .5s linear;
  -webkit-animation-fill-mode: forwards;
  animation-fill-mode: forwards;
}
.luckEgg .luckEggHalf {
  width: 100%;
  height: 50%;
  background-size: cover;
}
.luckEgg .luckEgg-bottom {
  background-position: 0 100%;
  background-repeat: no-repeat;
}
.luckEgg .luckEgg-top.show {
  -webkit-animation: showForTop 1s linear;
  animation: showForTop 1s linear;
  -webkit-animation-fill-mode: forwards;
  animation-fill-mode: forwards;
}
.luckEgg .luckEgg-bottom.show {
  -webkit-animation: showForBottom 1s linear;
  animation: showForBottom 1s linear;
  -webkit-animation-fill-mode: forwards;
  animation-fill-mode: forwards;
}
.lightRing,
.openLight {
  width: 120%;
  height: 120%;
  position: absolute;
  background-size: 100% 100%;
}
.lightRing {
  left: -10%;
  top: -10%;
  background-image: url("https://2018t3.oss-cn-beijing.aliyuncs.com/polar-bear/images/lightRing.png");
}
.openLight {
  left: -5%;
  top: -15%;
  -webkit-transform: scale(0.3);
  transform: scale(0.3);
  background-image: url("https://2018t3.oss-cn-beijing.aliyuncs.com/polar-bear/images/openLight.png");
}
.openLight.show {
  -webkit-animation: showForOpenLight 1s linear;
  animation: showForOpenLight 1s linear;
  -webkit-animation-fill-mode: forwards;
  animation-fill-mode: forwards;
}
